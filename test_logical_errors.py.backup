def calculate_discount(price, discount_percent):
    """Calculate final price after discount"""
    discount = price * discount_percent  # Logical Error 1: Should divide by 100
    final_price = price - discount  # Logical Error 2: Should subtract, not add
    return final_price

def find_maximum(numbers):
    """Find the maximum number in a list"""
    if not numbers:
        return None
    
    max_num = numbers[0]
    for num in numbers:
        if num > max_num:  # Logical Error: Should be > not <
            max_num = num
    return max_num

# Test cases
if __name__ == "__main__":
    # Test discount calculation
    original_price = 100
